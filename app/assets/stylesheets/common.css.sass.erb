/** Definiciones **/

@font-face
  font-family: 'Schuss'
  src: font-url('SchussHandITC_Bold.ttf')  format('truetype')
  font-weight: normal

@font-face
  font-family: 'OpenSans'
  src: font-url('OpenSans-Regular') format('truetype')
  font-weight: normal

$font-general: 'OpenSans', sans-serif
$titulo: 'Schuss'
$c-texto: #012b5a
$c-azul: #005cb8
$c-verde: #6abf4a
$c-rojo: #e55333
$b-rayas: 2px dashed #FFF

@mixin titulos
  font-family: $titulo
  font-size: 3em
  color: $c-verde
  margin-bottom: 0
@mixin titulos-home
  font-family: $titulo
  font-size: 40px
  clear: both
  margin-top: 0.5em 
  -ms-transform: skewX(40deg) /* IE 9 */
  -webkit-transform: skewX(20deg) /* Safari */
  -moz-transform: skewX(20deg)
  transform: skewX(20deg)
  a
    color: $c-azul
  
@mixin titulosotros
  font-family: $font-general
  color: $c-azul
@mixin titulossidebar
  font-family: $font-general
  background: $c-azul
  color: #FFF
  font-size: 0.9em
  text-transform: uppercase
  padding: 2px 5px

@mixin rayas
  border-top: $b-rayas
  border-bottom: $b-rayas
    
